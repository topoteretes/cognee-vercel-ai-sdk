	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"license": "MIT",
	"files": [
		"dist",
		"README.md"
	],
	"keywords": [
		"ai",
		"cognee",
		"vercel",
		"ai-sdk",
		"memory",
		"context",
		"llm",
		"openai",
		"anthropic"
	],
	"scripts": {
