		"typescript": "^5.9.3",
		"vitest": "^4.0.8"
	},
	"dependencies": {
		"@ai-sdk/anthropic": "^2.0.44",
		"@ai-sdk/openai": "^2.0.65",
		"ai": "^5.0.90",
		"debug": "^4.4.3",
		"dotenv": "^17.2.3",
